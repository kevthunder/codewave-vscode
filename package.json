{
	"name": "codewave-vscode",
	"displayName": "CodeWave",
	"description": "CodeWave : The text editor helper.",
	"version": "0.0.1",
	"publisher": "kevthunder",
	"repository": {
		"type": "git",
		"url": "git://github.com/kevthunder/codewave-vscode.git"
	},
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.codewaveActivate"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.codewaveActivate",
				"title": "Extend CodeWave Abreviation"
			}
		],
		"keybindings": [
			{
				"command": "extension.codewaveActivate",
				"key": "ctrl+shift+e",
				"mac": "shift+cmd+e",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"dependencies": {
		"codewave": "github:kevthunder/codewave"
	},
	"devDependencies": {
		"@types/mocha": "^5.2.7",
		"@types/node": "^10.12.21",
		"eslint": "^5.13.0",
		"typescript": "^3.3.1",
		"vscode": "^1.1.34",
		"webpack": "^4.41.4",
		"webpack-cli": "^3.3.10"
	}
}
